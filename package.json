  "name": "my-v0-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "cross-env ENABLE_LEADGEN_WORKER=1 next dev -p 3002",
    "typecheck:dlx": "pnpm dlx typescript tsc --noEmit",
    "migrate:all": "node ./scripts/migrate-all.mjs",
    "migrate:seed-users": "node ./scripts/seed-dev-users.mjs",
    "email:migrate": "node ./scripts/apply-email-migration.mjs",
    "crm:migrate": "node ./scripts/apply-crm-migration.mjs",
    "crm:migrate:docs": "node ./scripts/apply-crm-documents.mjs",
    "crm:migrate:surveys": "node ./scripts/apply-crm-surveys.mjs",
    "templates:seed": "node ./scripts/seed-templates.mjs",
    "templates:fetch:onlyoffice": "node ./scripts/fetch-onlyoffice-templates.mjs",
    "vscode:web": "code-server --bind-addr 127.0.0.1:3100 --auth none --disable-telemetry --disable-update-check --disable-file-downloads false --ignore-last-opened",
    "vscode:web:portable:win": "powershell -NoLogo -NoProfile -Command \"$ErrorActionPreference='Stop'; $url='https://github.com/coder/code-server/releases/download/v4.104.1/code-server-4.104.1-win32-x64.zip'; $zip=Join-Path $PWD 'code-server.zip'; $dest=Join-Path $PWD 'code-server-portable'; if(!(Test-Path $dest)){New-Item -ItemType Directory -Path $dest|Out-Null}; Invoke-WebRequest -Uri $url -OutFile $zip; Expand-Archive -Path $zip -DestinationPath $dest -Force; Remove-Item $zip; $exe=Get-ChildItem -Path $dest -Recurse -Filter code-server.exe | Select-Object -First 1 -ExpandProperty FullName; & $exe --bind-addr 127.0.0.1:3100 --auth none --disable-telemetry --disable-update-check --disable-file-downloads false --ignore-last-opened\"",
    "vscode:web:openvscode:win": "powershell -NoLogo -NoProfile -Command \"$ErrorActionPreference='Stop'; $ver='1.93.1.24173'; $url=('https://github.com/gitpod-io/openvscode-server/releases/download/openvscode-server-v' + $ver + '/openvscode-server-v' + $ver + '-win32-x64.zip'); $zip=(Join-Path $PWD 'openvscode.zip'); $dest=(Join-Path $PWD 'openvscode-portable'); if(!(Test-Path $dest)){New-Item -ItemType Directory -Path $dest|Out-Null}; Invoke-WebRequest -Uri $url -OutFile $zip; Expand-Archive -Path $zip -DestinationPath $dest -Force; Remove-Item $zip; $exe=(Get-ChildItem -Path $dest -Recurse -Filter server.cmd | Select-Object -First 1 -ExpandProperty FullName); & $exe --host 127.0.0.1 --port 3100 --without-connection-token\"",
    "vscode:web:proxy": "node ./scripts/vscode-proxy.mjs",
    "vscode:proxy:docker": "cross-env VSCODE_TARGET=http://127.0.0.1:3102 node ./scripts/vscode-proxy.mjs",
    "studio:migrate": "node ./scripts/apply-studio-migration.mjs",
    "dev:docker:vscode": "powershell -NoLogo -NoProfile -Command \"docker compose up -d vscode; Start-Sleep -Seconds 5; $env:VSCODE_TARGET='http://127.0.0.1:3102'; Start-Process -NoNewWindow -FilePath pnpm -ArgumentList 'vscode:web:proxy'; pnpm dev\"",
    "dev:all": "powershell -NoLogo -NoProfile -Command \"Start-Process -NoNewWindow -FilePath pnpm -ArgumentList 'vscode:web:portable:win'; Start-Sleep -Seconds 3; pnpm dev\"",
    "lint": "next lint",
    "start": "cross-env ENABLE_LEADGEN_WORKER=1 next start -p 3002",
    "slides:dev": "slidev presentation-examples/roadmap/slides.md",
    "slides:export": "slidev export presentation-examples/roadmap/slides.md --format pdf --output public/exports/roadmap.pdf",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules vitest run --config vitest.config.mts"
    ,"prisma:generate": "prisma generate"
    ,"prisma:migrate": "prisma migrate dev --name init"
    ,"prisma:deploy": "prisma migrate deploy"
    ,"worker:leadgen": "tsx workers/lead-extraction-worker.ts"
  },
